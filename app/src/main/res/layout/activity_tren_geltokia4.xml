<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_img_piedra"
    tools:context=".TrenGeltokia4">

    <ImageView
        android:id="@+id/textBocata"
        android:layout_width="384dp"
        android:layout_height="578dp"
        android:src="@drawable/ic_bocadillo_trengeltokia"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-19dp" />

    <ImageView
        android:id="@+id/arantza4"
        android:layout_width="235dp"
        android:layout_height="211dp"
        android:src="@drawable/arantza"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="447dp" />

    <TextView
        android:id="@+id/infoTren"
        android:layout_width="292dp"
        android:layout_height="259dp"
        android:text="@string/TrenGeltokia4_info"
        android:textSize="22dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="136dp" />

    <ImageView
        android:id="@+id/jarraitu4"
        android:layout_width="162dp"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_btn2"
        tools:layout_editor_absoluteX="214dp"
        tools:layout_editor_absoluteY="644dp"
        tools:ignore="MissingConstraints"
        android:onClick="continuar4"/>

    <TextView
        android:id="@+id/etiqueta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/jarraitu"
        android:textSize="25dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="251dp"
        tools:layout_editor_absoluteY="670dp"
        tools:ignore="MissingConstraints" />

</android.support.constraint.ConstraintLayout>